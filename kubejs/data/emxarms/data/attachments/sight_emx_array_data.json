{
  // 配件重量，单位 kg。影响持枪者的移速
  "weight": 0.2,
  // 对瞄准时间的影响，加数，单位为秒，可为负数
  "ads_addend": 0.12,
  // 爆头配置
   "head_shot":{
   "addend": 0.05
  // "multiplier": 1.5
   },
  // 对精准度的影响，加数，可为负数。负数代表精准度增加，正数表示精准度降低。
  "inaccuracy_addend": -0.05,
  // 对枪械后坐力的影响。
  "recoil_modifier": {
    // 竖直方向偏转削减的大小，范围-1~0
    "pitch": -0.04
  }
}